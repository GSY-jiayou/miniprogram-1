<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" style='width:100%;height:300rpx;'/>
      </swiper-item>
    </block>
  </swiper>

  <view class="weui-grids">  
    <view class="weui-grid" wx:for="{{routers}}" wx:key="name">  
      <navigator url="{{item.url}}">  
        <view class="weui-grid__icon">  
          <image src=" {{item.icon}}" mode="scaleToFill" />  
        </view>  
        <text class="weui-grid__label">{{item.text}}</text>  
      </navigator>  
    </view>  
  </view>  

  <view class='index-enter bgw'>
    <view class='enter-item'>
        <image src='/images/13.jpg'></image>
    </view>
    <view class='enter-item'>
        <image src='/images/14.jpg'></image>
    </view>
</view>
